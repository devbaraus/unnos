{
  "name": "ksi-uno",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently -k -c cyan,yellow,purple \"bun dev:bot\" \"bun dev:app\" \"bun dev:tailwind\"",
    "dev:bot": "bun --watch bot/index.ts",
    "dev:app": "bun --watch app/index.tsx",
    "dev:tailwind": "tailwindcss -i ./app/global.css -o ./app/public/global.css --watch",
    "db:push": "prisma db push --force-reset",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.5.3",
    "prisma": "^6.4.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/html": "^1.2.0",
    "@elysiajs/static": "^1.2.0",
    "@prisma/client": "^6.4.1",
    "@tailwindcss/cli": "^4.0.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.10",
    "valibot": "^1.0.0-rc.3"
  }
